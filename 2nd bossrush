confirmCheckMark.TextColor3 = Color3.fromRGB(0, 255, 0)
confirmCheckMark.Font = Enum.Font.GothamBold
confirmCheckMark.TextSize = 24
confirmCheckMark.Text = "‚úì"
confirmCheckMark.Visible = false

local confirmConn

local function startAutoConfirm()
	local acc = 0
	confirmConn = RunService.Heartbeat:Connect(function(dt)
		acc += dt
		if acc >= 0.1 then
			acc = 0
			local args = {
				{
					data = buffer.fromstring("\026\000\000"),
					references = {}
				}
			}
			local event = ReplicatedStorage:FindFirstChild("NetserEvent")
			if event then
				event:FireServer(unpack(args))
			end
		end
	end)
end

local function stopAutoConfirm()
	if confirmConn then
		confirmConn:Disconnect()
		confirmConn = nil
	end
end

confirmCheckbox.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		getgenv().autoConfirm = not getgenv().autoConfirm
		confirmCheckMark.Visible = getgenv().autoConfirm
		if getgenv().autoConfirm then
			startAutoConfirm()
		else
			stopAutoConfirm()
		end
	end
end)
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local player = Players.LocalPlayer
local event = ReplicatedStorage:WaitForChild("NetserEvent")

-- GUI ÏÉùÏÑ±
local gui = Instance.new("ScreenGui")
gui.Name = "NetserTriggerGui"
gui.ResetOnSpawn = false
gui.Parent = player:WaitForChild("PlayerGui")

local button = Instance.new("TextButton")
button.Size = UDim2.new(0, 200, 0, 50)
button.Position = UDim2.new(0.5, -100, 0.8, 0)
button.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
button.TextColor3 = Color3.fromRGB(255, 255, 255)
button.Text = "üîÅ Place Units"
button.Font = Enum.Font.GothamBold
button.TextSize = 18
button.Parent = gui

-- ÎìúÎûòÍ∑∏ Í¥ÄÎ†® Î≥ÄÏàò
local dragging = false
local dragInput = nil
local dragStart = nil
local startPos = nil

button.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = button.Position

		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

button.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if dragging and input == dragInput then
		local delta = input.Position - dragStart
		button.Position = UDim2.new(
			startPos.X.Scale,
			startPos.X.Offset + delta.X,
			startPos.Y.Scale,
			startPos.Y.Offset + delta.Y
		)
	end
end)

-- Netser Ïù¥Î≤§Ìä∏ Ìï®Ïàò
local function fireNetserEvents()
    local dataList = {
        "\029\000\004\002$\000\000\00093d12a38-3a4b-4a8f-9796-adb89df549c5\003\162J\172\196\000\000@@\129\134\153D\001\000\000\000\000\000\000\000\000\002&\000\000\000{d2a87b26-c778-45e5-8c02-e2050292d2c9}",
